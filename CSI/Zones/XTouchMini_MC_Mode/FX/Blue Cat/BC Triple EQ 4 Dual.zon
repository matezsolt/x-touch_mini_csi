Zone "VST: BC Triple EQ 4 (Dual) (Blue Cat Audio)"

	FocusedFXNavigator
	
	IncludedZones
		"Default FX Zone"
	IncludedZonesEnd    	

	LayerA												Reaper "_S&M_WNCLS3"			//SWS/S&M: Close all floating FX windows
	LayerA												Reaper "_S&M_HIDEFXCHAIN" //SWS/S&M: Hide FX chain windows for selected tracks
	LayerA												GoZone "Home"

	Rotary1												FXParam 5 "Low (1)"
	Rotary2												FXParam 6 "Mid (1)"
	Rotary3												FXParam 3 "Frequency (1)"
	Rotary4 											FXParam 4 "Bandwidth (1)"
	Rotary5												FXParam 7 "High (1)"
	Rotary6												FXParam 2 "Gain (1)"
	Rotary7												NoAction 
	Rotary8												NoAction

	InvertFB+ButtonUpper1 				FXParam 14 "Bypass" [ 0.0 1.0 ]
	ButtonUpper2 									FXParam 1 "Stereo Mode" [ 0.0 1.0 ]
	ButtonUpper3 									NoAction
	ButtonUpper4 									NoAction
	ButtonUpper5 									NoAction
	ButtonUpper6 									NoAction
	ButtonUpper7 									NoAction
	ButtonUpper8 									NoAction

	RotaryPush1 									Reaper "41749" //Track: Open/close UI for FX #1 on last touched track
	RotaryPush2 									Reaper "41750" //Track: Open/close UI for FX #2 on last touched track
	RotaryPush3 									Reaper "41751" //Track: Open/close UI for FX #3 on last touched track
